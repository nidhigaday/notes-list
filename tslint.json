{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "react-unused-props-and-state": true,
        "no-unused-variable": true,
        "ordered-imports": [
            true,
            {
                "import-sources-order": "lowercase-last",
                "named-imports-order": "lowercase-first"
            }
        ],
        "indent": [
            true,
            "spaces",
            4
        ]
    },
    "rulesDirectory": []
}